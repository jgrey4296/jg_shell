{
  "name": "js_shell",
  "version": "1.0.0",
  "description": "A Shell simulation / graph structure authoring program",
  "main": "server/serverMain, browser/shell.html",
  "scripts": {
    "start": "node_modules/.bin/webpack --config webpack.config.babel.js",
    "flow": "node_modules/.bin/flow",
    "test": "node_modules/.bin/mocha --check-leaks -c --prof --log-timer-events --require babel-polyfill --compilers js:babel-register",
    "pending": "node_modules/.bin/mocha --require babel-polyfill --compilers js:babel-register -R mocha-pending -c",
    "clean": "rm -r dist; rm -r docs;  exit 0",
    "doc": "node_modules/.bin/jsdoc ./src -r -d ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgrey4296/jg_shell.git"
  },
  "author": "John Grey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jgrey4296/jg_shell/issues"
  },
  "homepage": "https://github.com/jgrey4296/jg_shell#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-register": "^6.23.0",
    "blessed": "^0.1.81",
    "chai": "^3.5.0",
    "eslint": "^3.17.0",
    "eslint-loader": "^1.6.3",
    "flow-babel-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.40.0",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "mocha-pending": "^1.0.0",
    "parsimmon": "^1.2.0",
    "webpack": "^2.2.1"
  }
}
